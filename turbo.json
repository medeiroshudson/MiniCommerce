{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env"],
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true,
      "env": ["NODE_ENV", "BACKEND_PORT"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "start": {
      "cache": true,
      "persistent": true,
      "env": ["NODE_ENV", "BACKEND_PORT"]
    }
  }
}
